{
    "compilerOptions": {
      "target": "ES5",
      "lib": ["dom", "es2015"],
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true // Add this line
    },
    "include": [
      "src/**/*"  // Include all files in the src folder and its subfolders
    ]
  }
  